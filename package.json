{
  "name": "framework7-parcel-template",
  "version": "1.0.0",
  "description": "Framework7 starter app template bundle up by parcel",
  "main": "index.js",
  "scripts": {
    "start": "parcel public/index.html --port $PORT --no-hmr",
    "build": "parcel build index.html -d docs/ --public-url ./",
    "test": "echo \"Error: no test specified\" && exit 1",
    "init": "firebase init",
    "deploy": "firebase deploy",
    "login": "firebase login  --no-localhost",
    "use": "firebase use --add"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuillaumeBiton/framework7-parcel-template.git"
  },
  "keywords": [
    "framework7",
    "parceljs",
    "pwa"
  ],
  "author": "Guillaume Biton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GuillaumeBiton/framework7-parcel-template/issues"
  },
  "homepage": "https://github.com/GuillaumeBiton/framework7-parcel-template#readme",
  "devDependencies": {
    "parcel-bundler": "^1.12.3"
  },
  "dependencies": {
    "firebase-tools": "^7.0.0",
    "framework7": "^4.4.3",
    "framework7-icons": "^2.3.1",
    "material-design-icons": "^3.0.1"
  }
}
